{
  "name": "audio-warp",
  "version": "0.1.0",
  "description": "",
  "main": "src/app.ts",
  "bin": "out/pkg.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:tsc && npm run build:pkg && npm run build:rename",
    "build:clean": "npx del-cli out & npx del-cli dist",
    "build:tsc": "npx tsc",
    "build:rename": "npx renamer --find audio-warp --replace AudioWarp dist/*.exe",
    "build:pkg": "npx pkg package.json",
    "run-dev": "npx ts-node ."
  },
  "author": "Tim 'Piepmatz' Hesse",
  "license": "ISC",
  "devDependencies": {
    "@types/inquirer": "^7.3.3",
    "@types/node": "^16.4.13",
    "del-cli": "^4.0.1",
    "pkg": "^5.3.1",
    "renamer": "^3.2.1",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@discordjs/opus": "^0.5.3",
    "@discordjs/voice": "^0.5.6",
    "cli-select": "^1.1.2",
    "discord.js": "^13.0.1",
    "ffmpeg-static": "^4.4.0",
    "inquirer": "^8.1.2",
    "naudiodon": "^2.3.5",
    "prism-media": "^1.3.2",
    "sodium": "^3.0.2"
  },
  "pkg": {
    "assets": [
      "lib/ffmpeg.exe"
    ],
    "outputPath": "dist",
    "targets": [
      "node16-win-x64"
    ],
    "compress": true
  }
}
