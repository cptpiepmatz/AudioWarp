{
  "name": "audio-warp",
  "version": "1.1.0",
  "description": "A tool to warp your music input to Discord.",
  "keywords": [
    "warp",
    "audio",
    "radio",
    "discord",
    "bot"
  ],
  "main": "src/app.ts",
  "bin": "out/pkg.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:tsc && npm run build:icon && npm run build:pkg && npm run build:rename",
    "build:clean": "npx del-cli out & npx del-cli dist",
    "build:tsc": "npx tsc",
    "build:icon": "node scripts/icon.js",
    "build:pkg": "npx pkg package.json",
    "build:rename": "node scripts/rename.js",
    "run-dev": "npx ts-node ."
  },
  "author": "Tim 'Piepmatz' Hesse",
  "license": "MIT",
  "devDependencies": {
    "@types/discord-rich-presence": "^0.0.0",
    "@types/inquirer": "^7.3.3",
    "@types/node": "^16.6.1",
    "del-cli": "^4.0.1",
    "git-last-commit": "^1.0.0",
    "pkg": "^5.3.1",
    "rcedit": "^3.0.1",
    "renamer": "^3.2.1",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@discordjs/opus": "^0.5.3",
    "@discordjs/voice": "^0.6.0",
    "chalk": "^4.1.2",
    "cli-select": "^1.1.2",
    "discord-rich-presence": "^0.0.8",
    "discord.js": "^13.1.0",
    "ffmpeg-static": "^4.4.0",
    "inquirer": "^8.1.2",
    "naudiodon": "^2.3.5",
    "ora": "^5.4.1",
    "prism-media": "^1.3.2",
    "sodium": "^3.0.2"
  },
  "pkg": {
    "assets": [
      "lib/ffmpeg.exe",
      ".token",
      "node_modules/@discordjs/opus/**/*",
      "node_modules/sodium/**/*"
    ],
    "outputPath": "dist",
    "targets": [
      "node16-win-x64"
    ],
    "compress": "GZip"
  }
}
